	Dim returnValue
	Call cfGlUAIelementCheck(obj)
	If returnObject.Exist(strWaitTime2) Then
		statusFlag ="PASS"
		returnValue = returnObject.GetObjectProperty(propertyName)
		cfGlUAIGetObjectProperty = returnValue
