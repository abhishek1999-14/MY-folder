Public Function cfGlUAIGetAllProperties(obj)
	statusFlag ="FAIL"
	Set returnDictionary = CreateObject("Scripting.Dictionary")
	Call cfGlUAIelementCheck(obj)
	If returnDictionary.Count >0 Then
		statusFlag ="PASS"
		returnDictionary = returnObject.GetAllProperties
		cfGlUAIGetAllProperties = returnDictionary
	Else
		statusFlag ="FAIL"
	End If 
End Function

Public Function cfGlUAIGetObjectProperty(obj, propertyName)
