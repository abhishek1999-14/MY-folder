'''Function to set window context for AI objects
Function cfGlUAISetWindowContext(objWindow)
	statufFlag ="FAIL"
	Call retObject(objWindow)
	If returnObject.Exist(strWaitTime2) Then
		AIUtil.SetContext returnObject
		statusFlag ="PASS"
	Else
		statufFlag ="FAIL"
	End If
End Function

Function cfGlUAIElementCheck(obj)
	elementFlag="False"
	Call retObject(obj)
	If returnObject.Exist(strWaitTime2) Then
		If returnObject.CheckExists(true) Then
			elementFlag="True"
		Else
			elementFlag="False"
		End If
	Else
		elementFlag="False"
	End If
End Function

Public Function cfGlUAIElementClick(obj)
	Call cfGlUAIElementCheck(obj)
	statusFlag ="FAIL"
	If  elementFlag="True" Then
		returnObject.Click
	Set returnObject = Nothing
		statusFlag ="PASS"
		wait 1
	Else
		print "object doesnt exist or not visible"
		statusFlag="FAIL"
	End If
End Function

'Provide window object and .png file path of object to be clicked
Public Function cfGlUAIClickOnImageObject(objWindow,objPath)
	statusFlag ="FAIL"
	call cfGlUAIsetWindowContext(objWindow)
	AIUtil.RegisterCustomClass "object", objPath
	If AIUtil("object").Exist Then
		AIUtil("object").Click
		statusFlag ="PASS"
	Else
		statusFlag ="FAIL"
	End If
End Function

Public Function cfGlUAICaptureImage(objWindow,obj, FullFileName)
	statufFlag ="FAIL"
	call cfGlUAISetWindowContext(objWindow)
	Call cfGlUAIElementCheck(obj)
	If returnObject.Exist(strWaitTime2) Then
		returnObject.CaptureBitmap  FullFileName, True
		statusFlag ="PASS"
	Else
		statufFlag ="FAIL"
	End If
End Function

